{
    "name": "yr-group/laravel-zatca",
    "description": "Easy-to-use Laravel package for ZATCA (Saudi Arabian E-Invoicing) Phase 1 and Phase 2 integration",
    "type": "library",
    "keywords": [
        "laravel",
        "zatca",
        "saudi",
        "e-invoicing",
        "fatoora",
        "qr-code"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yassen Sayed",
            "email": "devysm2019@gmail.com",
            "homepage": "https://yassensayed.com/"
        },
        {
            "name": "Ramadan Ewis",
            "email": "devramdanewis@gmail.com",
            "homepage": "https://ramadanewais.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0",
        "salla/zatca": "^3.0",
        "prgayman/laravel-zatca": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "YrGroup\\LaravelZatca\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YrGroup\\LaravelZatca\\ZatcaServiceProvider"
            ],
            "aliases": {
                "Zatca": "YrGroup\\LaravelZatca\\Facades\\Zatca"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
